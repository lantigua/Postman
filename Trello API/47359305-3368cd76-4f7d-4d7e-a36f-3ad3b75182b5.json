		{
			"name": "Create Board",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "c7c78659-21c1-4e81-abfc-a661c3c7b046",
						"exec": [
							"let jsonData = pm.response.json();\r",
							"pm.environment.set(\"boardID\", jsonData.id);\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"let createdboardName = pm.environment.get(\"boardName\")\r",
							"pm.test(\"Correct Board Name\", function () {\r",
							"    pm.expect(jsonData.name).to.eql(createdboardName);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "bf22640c-2d12-4c32-bac8-81cd43a569bd",
						"exec": [
							"// Get the current version from an environment variable (or initialize it)\r",
							"let version = pm.environment.get(\"boardVersion\") || \"1.0\";\r",
							"\r",
							"// Split the version into major and minor parts\r",
							"let [major, minor] = version.split('.').map(Number);\r",
							"\r",
							"// Increment the minor version (e.g., 1.0 -> 1.1, 1.1 -> 1.2, etc.)\r",
							"minor += 1;\r",
							"\r",
							"// If minor reaches 10, reset to 0 and increment major (e.g., 1.9 -> 2.0)\r",
							"if (minor >= 10) {\r",
							"    minor = 0;\r",
							"    major += 1;\r",
							"}\r",
							"\r",
							"// Create the new version string\r",
							"let newVersion = `${major}.${minor}`;\r",
							"let boardName = `My Board - ${newVersion}`;\r",
							"\r",
							"// Save the new version and board name to environment variables\r",
							"pm.environment.set(\"boardVersion\", newVersion);\r",
							"pm.environment.set(\"boardName\", boardName);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
